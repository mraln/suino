<%- model_class = Parto -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:matriz_id) %>:</strong></dt>
  <dd><%= @parto.matriz_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:reprodutor_id) %>:</strong></dt>
  <dd><%= @parto.reprodutor_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cobertura) %>:</strong></dt>
  <dd><%= @parto.cobertura %></dd>
  <dt><strong><%= model_class.human_attribute_name(:previsao_parto) %>:</strong></dt>
  <dd><%= @parto.previsao_parto %></dd>
  <dt><strong><%= model_class.human_attribute_name(:data_parto) %>:</strong></dt>
  <dd><%= @parto.data_parto %></dd>
  <dt><strong><%= model_class.human_attribute_name(:qtde_filhos) %>:</strong></dt>
  <dd><%= @parto.qtde_filhos %></dd>
  <dt><strong><%= model_class.human_attribute_name(:mortalidade) %>:</strong></dt>
  <dd><%= @parto.mortalidade %></dd>
  <dt><strong><%= model_class.human_attribute_name(:total_desmamado) %>:</strong></dt>
  <dd><%= @parto.total_desmamado %></dd>
  <dt><strong><%= model_class.human_attribute_name(:data_desmame) %>:</strong></dt>
  <dd><%= @parto.data_desmame %></dd>
  <dt><strong><%= model_class.human_attribute_name(:retorno_cio) %>:</strong></dt>
  <dd><%= @parto.retorno_cio %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              matriz_partos_path(@matriz), :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_matriz_parto_path(@matriz, @parto), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              matriz_parto_path(@matriz, @parto),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
